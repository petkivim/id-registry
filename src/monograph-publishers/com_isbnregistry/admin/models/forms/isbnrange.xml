<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_isbnregistry/models/rules">	
    <fieldset>
        <field
            name="id"
            type="hidden"
        />
		<field 
			name="prefix" 
			type="list" 
			default="" 
			class="validate-prefix"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_PREFIX_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_PREFIX_DESC"
			>
			<option value="">COM_ISBNREGISTRY_FIELD_SELECT</option>
			<option value="978">978</option>
			<option value="979">979</option>
		</field>	
		<field 
			name="lang_group" 
			type="list" 
			default="" 
			class="validate-langgroup"
			required="true"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_LANG_GROUP_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_LANG_GROUP_DESC"
			>
			<option value="">COM_ISBNREGISTRY_FIELD_SELECT</option>
			<option value="951">951</option>
			<option value="952">952</option>
		</field>			
        <field
            name="range_begin"
            type="text"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_RANGE_BEGIN_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_RANGE_BEGIN_DESC"
            size="20"
            class="inputbox validate-rangebegin"
			required="true"
            default=""
        />
        <field
            name="range_end"
            type="text"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_RANGE_END_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_RANGE_END_DESC"
            size="20"
            class="inputbox validate-rangeend"
			required="true"
            default=""
        />	
		<field 
			name="is_active" 
			type="radio" 
			default="1" 
			required="true" 
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_IS_ACTIVE_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_IS_ACTIVE_DESC"
			>
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>		
        <field
            name="free"
            type="hidden"
        />	
		<field 
			name="free_lbl"
			type="spacer" 
			label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_FREE_LABEL" 
			description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_FREE_DESC" 
		/>
        <field
            name="taken"
            type="hidden"
        />	
		<field 
			name="taken_lbl"
			type="spacer" 
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_TAKEN_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_TAKEN_DESC"
		/>
        <field
            name="canceled"
            type="hidden"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CANCELED_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CANCELED_DESC"
        />		
        <field
            name="next"
            type="hidden"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_NEXT_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_NEXT_DESC"
            size="20"
            readonly="true"
            default=""
        />			
        <field 
            name="created"
            type="calendar"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CREATED_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CREATED_DESC"
            readonly="true"
            class="readonly"
            format="%Y-%m-%d %H:%M:%S"
            filter="server_utc"
        />
        <field
            name="created_by"
            type="text"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CREATED_BY_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_CREATED_BY_DESC"
            size="20"
            readonly="true"
            class="readonly"
        />	
        <field 
            name="modified"
            type="calendar"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_MODIFIED_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_MODIFIED_DESC"
            readonly="true"
            class="readonly"
            format="%Y-%m-%d %H:%M:%S"
            filter="server_utc"
        />
        <field
            name="modified_by"
            type="text"
            label="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_MODIFIED_BY_LABEL"
            description="COM_ISBNREGISTRY_ISBN_RANGE_FIELD_MODIFIED_BY_DESC"
            size="20"
            readonly="true"
            class="readonly"
        />
    </fieldset>
</form>